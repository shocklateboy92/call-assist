{
  "name": "call-assist-matrix-plugin",
  "version": "0.1.0",
  "description": "Matrix plugin for Call Assist",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "proto": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/proto_gen --proto_path=../../proto ../../proto/*.proto"
  },
  "dependencies": {
    "nice-grpc": "^2.1.8",
    "matrix-js-sdk": "^37.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-proto": "^2.7.5"
  }
}